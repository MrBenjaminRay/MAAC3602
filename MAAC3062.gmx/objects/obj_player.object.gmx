            projectile.xDest = mouse_x;
            projectile.yDest = mouse_y;
            projectile.damage = attackDamage;
            projectile.isEnemyProjectile = false;
            
            scr_create_explosion(projX, projY, 'Flash');
            
            attackRateCounter = 0;
        }
    }
}
