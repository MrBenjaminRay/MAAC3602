            <kind>1</kind>
            <string>if (global.state == "Paused") {
    return 0;
}

// LEFT &amp; RIGHT MOVEMENT:
var isLeftPressed = keyboard_check(ord('a')) || keyboard_check(ord('A'));
var isRightPressed = keyboard_check(ord('d')) || keyboard_check(ord('D'));

if (isLeftPressed) {
    scr_entity_moveLeft();
    animationSpeed = 0.33;
    image_speed = 0.33;
    image_xscale = 1;
    if (isJumping) {
        sprite_index = spr_player_jump_left;
    } else {
        sprite_index = spr_player_runs_left_with_weapon;
    }
    facingDirection = -1; // Set this so we know player is facing left
}

if (isRightPressed) {
    scr_entity_moveRight();
    animationSpeed = 0.33;
    image_speed = 0.33;
    image_xscale = 1;
    if (isJumping) {
        sprite_index = spr_player_jump_right;
    } else {
        sprite_index = spr_player_runs_right_with_weapon;
    }
    facingDirection = 1; // Set this so we know player is facing right
}

if(!(isRightPressed || isLeftPressed)) {
    animationSpeed = 0;
    image_speed = 0;
    image_xscale = 1;
    if (isJumping) {
        if (sprite_index != spr_player_jump_right &amp;&amp; sprite_index != spr_player_jump_left) {
            // Jumping while standing still but not showing a jumping sprite,
            // so pick left or right jumping sprite at random
            if (irandom(1) == 1) {
                sprite_index = spr_player_jump_right;
            } else {
                sprite_index = spr_player_jump_left;
            }        
        }
    } else {
        // Player is standing still, so show correct sprite.
        // Sprite will be updated with firing stance sprites when mouse being held.
        if (facingDirection == 1) {
            // Player standing facing right
            sprite_index = spr_player_runs_right_with_weapon;
            image_index = 4; // Remove this when we have the standing facing right sprite
        } else if (facingDirection == -1) {
            // Player standing facing left
            sprite_index = spr_player_runs_left_with_weapon;
            image_index = 6; // Remove this when we have the standing facing right sprite
        }
    }
}

