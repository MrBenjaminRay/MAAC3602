                scr_energy_change(((global.towers[5, 1] * global.towerUpgradeLevel[5]) * towerHealthPct) * costRecoveryPercentage);
                break;
            }
        }
    
        with (intersectedTower) {
            global.tutorial_part3 = false;
            with(obj_messageBox){
